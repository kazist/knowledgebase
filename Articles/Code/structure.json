    "query": {
        "from": {
            "table": "Knowledgebase\\Articles\\Code\\Tables\\Articles",
            "alias": "ka",
            "name": "#__knowledgebase_articles"
        },
        "select": [
            "ka.id",
            "ka.title",
            "ka.category_id",
            "kc.title AS category_id_title",
            "ka.group_id",
            "ug.name AS group_id_name",
            "ka.article",
            "ka.published",
            "ka.featured",
            "ka.has_comments",
            "ka.hit",
            "ka.created_by",
            "uu.name AS created_by_name",
            "ka.date_created",
            "ka.modified_by",
            "uu_4760.name AS modified_by_name",
            "ka.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Knowledgebase\\Categories\\Code\\Tables\\Categories",
                "alias": "kc",
                "name": "#__knowledgebase_categories",
                "on": "ka.category_id = kc.id"
            },
            {
                "table": "Users\\Groups\\Code\\Tables\\Groups",
                "alias": "ug",
                "name": "#__users_groups",
                "on": "ka.group_id = ug.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ka.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_4760",
                "name": "#__users_users",
                "on": "ka.modified_by = uu_4760.id"
            }
        ]
    }
